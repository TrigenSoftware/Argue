{
  "name": "argue-cli",
  "version": "2.0.0-alpha",
  "description": "Node.js CLI arguments parser.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/Argue.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/Argue/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "checkDeps": "npm-check -s; exit 0",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "test": "npm run checkDeps && npm run lint && npm run jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "strict",
    "cli",
    "arguments",
    "parser",
    "console",
    "terminal"
  ],
  "devDependencies": {
    "coveralls": "^3.0.1",
    "jest": "^23.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.61.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-tslint": "^0.1.34",
    "rollup-plugin-typescript2": "^0.15.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-trigen": "^3.0.5",
    "typescript": "^2.9.2"
  },
  "files": [
    "lib"
  ]
}
